{
    "workbench.iconTheme": "material-icon-theme",
    "python.pythonPath": "python3",
    "terminal.integrated.rendererType": "dom",
    "git.autofetch": true,

    "[ruby]": {
        "editor.tabSize": 2,
        "editor.fontFamily": "Fira Code",
        "editor.fontLigatures": true
    },
    "window.zoomLevel": 0,
    "editor.rulers": [
        80
    ],
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Gruvbox Dark Soft",
        
    "workbench.colorCustomizations": {
        "sideBar.background": "#f2e5bc",
        "sideBar.foreground": "#3c3836",
        "selection.background": "#cc241d",
        "editor.focusedStackFrameHighlightBackground": "#d79921",
        "list.activeSelectionForeground": "#d79921",
        "input.background": "#3c3836",
        // "input.foreground": "#ff0000",
        // "input.placeholderForeground": "#ff0000",
        "editorSuggestWidget.border": "#f2e5bc",
        "editorSuggestWidget.selectedBackground": "#5c534f",
        "list.activeSelectionBackground": "#3c3836",
        "list.inactiveSelectionForeground": "#3c3836",
        "list.inactiveSelectionBackground": "#c7ae7b",
        "sideBarTitle.foreground": "#3c3836",
        "sideBarSectionHeader.foreground": "#3c3836",
        "tab.inactiveBackground": "#f2e5bc",
        "tab.inactiveForeground": "#3c3836",
        "tab.activeBackground": "#3c3836",
        "tab.activeForeground": "#f2e5bc",
        "tab.border": "#3c3836",
        "activityBar.background": "#f2e5bc",
        "activityBar.foreground": "#3c3836",
        "editorGroupHeader.tabsBackground": "#f2e5bc",
        "panel.border": "#f2e5bc"
    }
}
